    "@types/graphql": "^14.5.0",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.5.34",
    "@types/rosie": "^0.0.37",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "apollo-server-testing": "^2.9.13",
    "chai": "^4.2.0",
    "faker": "^4.1.0",
    "mocha": "^7.1.1",
    "mockgoose": "^8.0.4",
    "rosie": "^2.0.1",
