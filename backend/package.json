{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "start": "tsnd --transpile-only -- index.ts",
    "test": "mocha -r ts-node/register --timeout 30000 --file ./tests/dbMock.ts tests/**/*.test.ts",
    "test-watch": "yarn test --watch -watch-extensions ts",
    "lint": "eslint ./src/**",
    "prettier-check": "prettier --check ./**/*.ts",
    "format": "prettier --write ./**/*.ts"
  },
  "dependencies": {
    "apollo-server": "^2.9.13",
    "google-auth-library": "^5.7.0",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "mongoose": "^5.8.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/faker": "^4.1.8",
    "@types/graphql": "^14.5.0",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.34",
    "@types/rosie": "^0.0.37",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "apollo-server-testing": "^2.9.13",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "faker": "^4.1.0",
    "mocha": "^6.2.2",
    "mockgoose": "^8.0.4",
    "prettier": "^1.19.1",
    "rosie": "^2.0.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.3"
  },
  "nodemonConfig": {
    "execMap": {
      "js": "node",
      "ts": "node --require ts-node/register"
    },
    "watch": [
      "src"
    ],
    "ext": "js,ts"
  }
}
